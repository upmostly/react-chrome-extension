{
  "name": "react-chrome-extension-boilerplate",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "gmeta": "^1.2.1",
    "http2": "^3.3.7",
    "jquery": "3.4.1",
    "meta-extractor": "^2.1.0",
    "metadata-scraper": "^0.2.33",
    "node-fetch": "^2.6.1",
    "open-graph": "^0.2.6",
    "open-graph-scraper": "^4.9.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-draggable": "4.0.3",
    "react-feather": "2.0.3",
    "react-hot-keys": "^2.6.2",
    "react-hotkeys": "^2.0.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
